<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2" xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee 
    http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">


	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>toSummerhousesSearch</from-outcome>
			<to-view-id>/summerhouseSearch.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>toUsers</from-outcome>
			<to-view-id>/allusers.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/allusers.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>toUserInfo</from-outcome>
			<to-view-id>/userinfo.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>toSummer</from-outcome>
			<to-view-id>/summerhouses.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>goToReservation</from-outcome>
			<to-view-id>/reservationPaymentProcess.xhtml</to-view-id>
			<redirect/>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>toSignCheck</from-outcome>
			<to-view-id>/signin.xhtml?faces-redirect=true</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/signin.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>goToLoggedPage</from-outcome>
			<to-view-id>/index.xhtml?faces-redirect=true</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>toPayment</from-outcome>
			<to-view-id>/paymentProcess.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>toMyReservations</from-outcome>
			<to-view-id>/myReservations.xhtml</to-view-id>
			<redirect/>
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>toSumerhousesAdministration</from-outcome>
			<to-view-id>/summerhouse-admin.xhtml</to-view-id>
			<redirect/>
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>toUsersAdministration</from-outcome>
			<to-view-id>/user-admin.xhtml</to-view-id>
			<redirect/>
		</navigation-case>
	</navigation-rule>

</faces-config>